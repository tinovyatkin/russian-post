{
  "name": "russian-post",
  "description": "Russian Post APIs client for Node.JS written in TypeScript",
  "license": "MIT",
  "author": "Konstantin Vyatkin <tino@vtkn.io>",
  "homepage": "https://github.com/tinovyatkin/russian-post#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinovyatkin/russian-post.git"
  },
  "bugs": {
    "url": "https://github.com/tinovyatkin/russian-post/issues"
  },
  "version": "1.0.2",
  "main": "dist/index.js",
  "files": [
    "dist/**/*.d.ts",
    "dist/**/*.js"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ext=ts ./src",
    "prepublishOnly": "yarn test && yarn build",
    "test": "node -r dotenv/config --trace-warnings --unhandled-rejections=strict --expose-gc node_modules/jest-cli/bin/jest --detectOpenHandles --logHeapUsage"
  },
  "dependencies": {
    "camel-case": "4.1.1",
    "strong-soap": "2.0.0"
  },
  "devDependencies": {
    "@types/jest": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "dotenv": "8.2.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-node": "11.1.0",
    "husky": "latest",
    "jest": "26.0.1",
    "jest-circus": "26.0.1",
    "jest-sonar": "0.2.8",
    "lint-staged": "latest",
    "npm-package-json-lint": "latest",
    "prettier": "latest",
    "prettier-package-json": "latest",
    "ts-jest": "latest",
    "typescript": "latest",
    "weak-napi": "2.0.1"
  },
  "keywords": [
    "api",
    "russian post",
    "tracking",
    "Почта России"
  ],
  "engines": {
    "node": ">=10.20",
    "yarn": ">=1.19"
  }
}
